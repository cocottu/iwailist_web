# Production Environment
# Firebase Console から取得した本番環境の設定値を入力してください

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_prod_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=cocottu-iwailist
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_prod_sender_id
VITE_FIREBASE_APP_ID=your_prod_app_id

# Environment
VITE_APP_ENV=production

# Basic Authentication (disabled for production)
VITE_BASIC_AUTH_ENABLED=false
VITE_BASIC_AUTH_USERNAME=
VITE_BASIC_AUTH_PASSWORD=
VITE_BASIC_AUTH_REALM=Iwailist Production
VITE_BASIC_AUTH_FORCE=false
# VITE_BASIC_AUTH_ALLOWED_ENVS=development,staging
